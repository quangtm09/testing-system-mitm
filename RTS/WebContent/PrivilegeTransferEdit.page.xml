<?xml version="1.0" encoding="UTF-8"?>
<page login-required="true" no-conversation-view-id="/PrivilegeTransferList.xhtml"
	xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd">
	<begin-conversation flush-mode="MANUAL" join="true" />
	<param name="isUpdate" value="#{privilegeTransferBLO.updating}" />
	<param name="selectedPT" value="#{privilegeTransferBLO.idOfSelectedPT}" />
	<action execute="#{privilegeTransferBLO.fillValueForUserList()}" />
	<action execute="#{privilegeTransferHome.wire}" />
	<navigation from-action="#{privilegeTransferBLO.save()}">
		<rule if-outcome="persisted">
			<end-conversation />
			<redirect view-id="/PrivilegeTransferList.xhtml" />
		</rule>
		<rule if-outcome="failed">
			<end-conversation />
			<redirect view-id="/PrivilegeTransferEdit.xhtml" />
		</rule>
	</navigation>
	<navigation from-action="#{privilegeTransferBLO.update()}">
		<rule if-outcome="updated">
			<end-conversation />
			<redirect view-id="/PrivilegeTransferList.xhtml" />
		</rule>
		<rule if-outcome="failed">
			<end-conversation />
			<redirect view-id="/PrivilegeTransferEdit.xhtml" />
		</rule>
	</navigation>
</page>
