<?xml version="1.0" encoding="UTF-8"?>
<page login-required="true" no-conversation-view-id="/InterviewScheduleList.xhtml"
	xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd">
	<restrict>
		#{(interviewScheduleHome.checkAddableTechnicalResult(interviewScheduleHome.instance)) and not identity.hasRole('disabledUser')}
	</restrict>
	<begin-conversation flush-mode="auto" join="true" />
	<action execute="#{interviewScheduleHome.wire()}" />
	<action execute="#{technicalResultHome.wire()}" />
	<action execute="#{technicalResultHome.onloadTechnicalResult('Y')}" on-postback="false" />		
		
	<param name="interviewScheduleItsInterviewScheduleId"
		value="#{interviewScheduleHome.interviewScheduleItsInterviewScheduleId}" />
	<param name="fromView" value="#{interviewScheduleHome.fromView}" />
	<navigation from-action="#{technicalResultHome.giveTechnicalFeedback()}">
		<rule if-outcome="persisted">
			<end-conversation before-redirect="true" />
			<redirect include-page-params="false" view-id="/InterviewScheduleList.xhtml" />
		</rule>
		<rule if-outcome="failure">
			<render view-id="/TechnicalResultEdit.xhtml" />
		</rule>
	</navigation>
	<navigation from-action="#{technicalResultHome.updateTechnicalFeedback()}">
		<rule if-outcome="updated">
			<end-conversation before-redirect="true" />
			<redirect include-page-params="false" view-id="/InterviewScheduleList.xhtml" />
		</rule>
		<rule if-outcome="failure">
			<redirect view-id="/TechnicalResultEdit.xhtml" />
		</rule>
	</navigation>
</page>
