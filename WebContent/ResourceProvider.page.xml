<?xml version="1.0" encoding="UTF-8"?>
<page login-required="true" xmlns="http://jboss.com/products/seam/pages"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd">
	<param name="candidateId" value="#{resources.candidateId}" />
	<param name="recruitRequestId" value="#{resources.recruitRequestId}" />
	<action execute="#{resources.checkResourceAvailable()}" />
	<navigation from-action="#{resources.checkResourceAvailable()}">
		<rule if-outcome="unavailable">
			<redirect view-id="/error.xhtml">
				<message>Resource is not available.</message>
			</redirect>
		</rule>
	</navigation>
</page>
