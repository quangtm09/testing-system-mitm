<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
	no-conversation-view-id="/ia/GroupList.xhtml" login-required="true">
	<restrict>#{identity.hasRole('admin') and not identity.hasRole('disabledUser')}</restrict>
	<begin-conversation flush-mode="auto" join="true" />

	<action execute="#{technicalResultLineAttributeHome.onloadAttributes()}" on-postback="false"/>
	<action execute="#{technicalResultLineAttributeHome.wire()}" />
	<param name="fromView" value="#{technicalResultLineAttributeHome.fromView}"/>
	
	<param name="technicalResultLineAttributeId"
		value="#{technicalResultLineAttributeHome.technicalResultLineAttributeId}" />
		
		
	<navigation from-action="#{technicalResultLineAttributeHome.createAttribute()}">
		<rule if-outcome="persisted"> 
			<end-conversation before-redirect="true"/>
			<redirect include-page-params="false" view-id="/ia/GroupList.xhtml"/>
		</rule>	
		
		<rule if-outcome="failure"> 
			<render view-id="/ia/GroupList.xhtml" />
		</rule>	
	</navigation>
	
</page>