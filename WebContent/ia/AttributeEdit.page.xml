<?xml version="1.0" encoding="UTF-8"?>
<page login-required="true"
 no-conversation-view-id="/ia/AttributeList.xhtml"
 xmlns="http://jboss.com/products/seam/pages"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd">
 <restrict>#{identity.hasRole('admin') and not identity.hasRole('disabledUser')}</restrict>
 <begin-conversation flush-mode="auto" join="true"/>
 <action execute="#{technicalResultLineAttributeHome.wire()}"/>
 <param name="fromView" value="#{technicalResultLineAttributeHome.fromView}"/>
 <param name="technicalResultLineAttributeId" value="#{technicalResultLineAttributeHome.technicalResultLineAttributeId}"/>
 <navigation from-action="#{technicalResultLineAttributeHome.updateAttribute()}">
  <rule if-outcome="updated">
   <end-conversation before-redirect="true"/>
   <redirect include-page-params="false" view-id="/ia/AttributeList.xhtml"/>
  </rule>
 </navigation>
 <navigation from-action="#{technicalResultLineAttributeHome.deleteAttribute()}">
  <rule if-outcome="removed">
   <end-conversation before-redirect="true"/>
   <redirect include-page-params="false" view-id="/ia/AttributeList.xhtml"/>
  </rule>
 </navigation>
</page>
