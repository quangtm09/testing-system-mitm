<?xml version="1.0" encoding="UTF-8"?>
<page login-required="true" no-conversation-view-id="/ia/GroupList.xhtml"
	xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd">
	<restrict>#{identity.hasRole('admin') and not identity.hasRole('disabledUser')}</restrict>
	<begin-conversation flush-mode="auto" join="true" />
	<action execute="#{technicalResultGroupHome.wire()}" />
	<action execute="#{technicalResultGroupHome.onloadLines()}" on-postback="false" />
	<action execute="#{technicalResultGroupHome.onloadSelectedLines()}" on-postback="false" />
	
	<param name="fromView" value="#{technicalResultGroupHome.fromView}"/>
	<param name="technicalResultGroupId" value="#{technicalResultGroupHome.technicalResultGroupId}" />
	
	<navigation from-action="#{technicalResultGroupHome.updateTechnicalResultGroup()}">
		<rule if-outcome="updated">
			<end-conversation before-redirect="true"/>
			<redirect view-id="/ia/GroupList.xhtml" include-page-params="false"/>
		</rule>
	</navigation>
	<navigation from-action="#{technicalResultGroupHome.removeTechnicalResultGroup()}">
		<rule if-outcome="removed">
			<end-conversation before-redirect="true"/>
			<redirect view-id="/ia/GroupList.xhtml" include-page-params="false"/>
		</rule>
	</navigation>
</page>