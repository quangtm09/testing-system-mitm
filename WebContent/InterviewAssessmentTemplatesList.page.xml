<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
	no-conversation-view-id="/InterviewAssessmentTemplatesList.xhtml" login-required="true">
	<restrict>#{identity.hasRole('admin') and not identity.hasRole('disabledUser')}</restrict>
	<begin-conversation flush-mode="auto" join="true" />

	<action execute="#{interviewAssessmentTemplatesHome.onloadAssessmentTemplates}" on-postback="false"/>
	<action execute="#{interviewAssessmentTemplatesHome.wire()}" />
	<param name="fromView" />
	
	<param name="trtsInterviewAssessmentTemplatesId"
		value="#{interviewAssessmentTemplatesHome.trtsInterviewAssessmentTemplatesId}" />
		
		
	<navigation from-action="#{interviewAssessmentTemplatesHome.createTemplate()}">
		<rule if-outcome="persisted"> 
			<end-conversation before-redirect="true"/>
			<redirect include-page-params="false" view-id="/InterviewAssessmentTemplatesList.xhtml" />
		</rule>	
		
		<rule if-outcome="failure"> 
			<render view-id="/InterviewAssessmentTemplatesList.xhtml" />
		</rule>	
	</navigation>
	
</page>