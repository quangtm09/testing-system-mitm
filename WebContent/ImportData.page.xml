<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
	login-required="true">
	<restrict>
		 #{identity.hasRole('hr') and not identity.hasRole('disabledUser')}
	</restrict>
	<navigation from-action="#{importExcelData.uploadFile()}">
		 <rule if-outcome="success">
			 <redirect view-id="/CandidateList.xhtml" include-page-params="false"/>
		 </rule>
		 <rule if-outcome="failure">
			<redirect view-id="/ImportData.xhtml" include-page-params="false"/>
		</rule>
	 </navigation>
</page>